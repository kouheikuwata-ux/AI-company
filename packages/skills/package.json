{
  "name": "@ai-company-os/skills",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist .turbo",
    "skills:generate": "npx tsx scripts/generate-registry.ts",
    "skills:validate": "npx tsx scripts/validate-skills.ts"
  },
  "dependencies": {
    "@ai-company-os/skill-spec": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "glob": "^10.3.10",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
